<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".NextSplash"
    android:background="@drawable/greenery11">

    <ImageView
        android:id="@+id/iv1"
        android:background="@drawable/f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="69dp"
        android:layout_marginTop="171dp"
        />

    <ImageView
        android:id="@+id/iv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/iv1"
        android:layout_toRightOf="@+id/iv1"
        android:background="@drawable/i" />

    <ImageView
        android:id="@+id/iv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/iv1"
        android:layout_toRightOf="@+id/iv2"
        android:background="@drawable/n" />

    <ImageView
        android:id="@+id/iv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/iv3"
        android:layout_toRightOf="@+id/iv3"
        android:background="@drawable/a" />

    <ImageView
        android:id="@+id/iv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/iv3"
        android:layout_toRightOf="@+id/iv4"
        android:background="@drawable/l" />

    <ImageView
        android:id="@+id/iv6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/iv1"
        android:layout_below="@+id/iv3"
        android:layout_marginTop="24dp"
        android:background="@drawable/l1" />

    <ImageView
        android:id="@+id/iv7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/iv6"
        android:layout_toRightOf="@+id/iv6"
        android:background="@drawable/v" />

    <ImageView
        android:id="@+id/iv8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/iv6"
        android:layout_toRightOf="@+id/iv2"
        android:background="@drawable/v" />

    <ImageView
        android:id="@+id/iv9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/iv4"
        android:layout_alignTop="@+id/iv8"
        android:background="@drawable/v" />

    <ImageView
        android:id="@+id/iv10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/iv9"
        android:layout_toRightOf="@+id/iv9"
        android:background="@drawable/v" />

</RelativeLayout>