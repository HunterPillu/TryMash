<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".NextSplash"
    android:background="@drawable/greenery11">
    <ImageView
        android:background="@drawable/v"
        android:id="@+id/iv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/iv1"
        android:layout_toRightOf="@+id/iv1"
        />

    <ImageView
        android:id="@+id/iv3"
        android:background="@drawable/x"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/iv2"
        android:layout_toRightOf="@+id/iv2"

        />

    <ImageView
        android:id="@+id/iv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/iv2"
        android:layout_toRightOf="@+id/iv3"
        android:background="@drawable/t"

        />

    <ImageView
        android:id="@+id/iv6"
        android:background="@drawable/v"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/iv5"
        android:layout_toRightOf="@+id/iv5"
        />

    <ImageView
        android:id="@+id/iv7"
        android:background="@drawable/v"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/iv6"
        android:layout_toRightOf="@+id/iv6"
        />

    <ImageView
        android:id="@+id/iv8"
        android:background="@drawable/v"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/iv5"
        android:layout_toRightOf="@+id/iv7"
        />

    <ImageView
        android:id="@+id/iv9"
        android:background="@drawable/l2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/iv8"
        android:layout_toRightOf="@+id/iv8"
        />

    <ImageView
        android:id="@+id/iv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/iv8"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="66dp"
        android:background="@drawable/n" />

    <ImageView
        android:id="@+id/iv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/iv1"
        android:layout_centerVertical="true"
        android:background="@drawable/l1" />

</RelativeLayout>