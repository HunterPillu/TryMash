<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.mobifly.com.studio_trymash" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            >

        </activity>
        <activity
            android:name=".SplashScreen.FinalSplash"
            android:label="@string/title_activity_final_splash"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".SplashScreen.NextSplash"
            android:label="@string/title_activity_next_splash"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Activity.EndActivity"
            android:label="@string/title_activity_end"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Activity.FirstActivity"
            android:label="@string/title_activity_first"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Activity.SoundSettingsActivity"
            android:label="@string/title_activity_sound_settings"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Activity.HighScoreActivity"
            android:label="@string/title_activity_high_score"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Activity.HelpActivity"
            android:label="@string/title_activity_help"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".SplashScreen.Splash"
            android:label="@string/title_activity_splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
